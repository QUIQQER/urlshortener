<?xml version="1.0" encoding="UTF-8"?>
<database>
    <global>
        <table name="urlshortener">
            <field type="BIGINT(20)">id</field>
            <field type="VARCHAR(100) NULL">shortened</field>
            <field type="TEXT NULL">url</field>
            <field type="TEXT NULL">host</field>
            <field type="TEXT NULL">title</field>
            <field type="TEXT NULL">description</field>
            <field type="TEXT NULL">params</field>
            <field type="INT(20) NULL">count</field>

            <primary>id</primary>
            <auto_increment>id</auto_increment>
        </table>
    </global>
</database>
